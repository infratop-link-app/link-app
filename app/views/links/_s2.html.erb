<% s2_links.each do |s2_link| %>
    <div class="link-box" data-item_id="<%= s2_link.id %>">
        <p class="icon_button">
            <%= link_to edit_link_path(s2_link) do%>
                <i class="fa fa-edit"></i>
            <% end %>
            <span class="not_favorited" id="fav_<%= s2_link.id %>" data-id="<%= s2_link.id %>">
                <i class="fa fa-bookmark-o book-mark-icon"></i>
            </span>
        </p>

        <a href="<%= s2_link.url %>">
            <%= image_tag 's2.png', size: '40x40', class: "link-img" %>
            <p class="link-title"><%= s2_link.title %></p>
        </a>
    </div>
<% end %>